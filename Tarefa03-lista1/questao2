e)#include <studio.h>
int main(){
float largura,altura,area,tinta_necessaria;
int latas;
printf('digite a largura da parede em metros:");
scanf("%f",&largura);
printf('digite a altura da parede em metros:");
scanf("%f",&altura);
area=largura*altura;
tinta_necessaria=(area*0,3)/2;
latas=(int)
(tinta_necessaria+0,99);
printf("voce pricisara de %d latas de tinta.\n,latas);
return 0;
}
