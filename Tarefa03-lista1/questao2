g)#include <stdio.h>

int main() {
double P, V, T, M;
printf("Digite a pressão (P) em psi: ");
scanf("%lf", &P);
printf("Digite o volume (V) em pés cúbicos: ");
scanf("%lf", &V);
printf("Digite a temperatura (T) em graus Fahrenheit: ");
scanf("%lf", &T);
M = ((P * V) / (0.37 * (T + 460));
printf("A massa de ar do pneu é: %.2lf\n", M);
return 0;}

