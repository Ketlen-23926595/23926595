g)#include <stdio.h>

int main() {
    float P, V, T, M;
    printf("Digite a pressão (P) em psi: ");
    scanf("%f", &P);
    printf("Digite o volume (V) em pés cúbicos: ");
    scanf("%f", &V);
    printf("Digite a temperatura (T) em graus Fahrenheit: ");
    scanf("%f", &T);
    M = (P * V) / (0.37 * (T + 460));
    printf("A massa de ar do pneu é: %.2f\n", M);
    return 0;
}
h)#include <stdio.h>

int main() {
    int a1, r, n, an;

    printf("Digite o primeiro termo da PA (a1): ");
    scanf("%d", &a1);

    printf("Digite a razão da PA (r): ");
    scanf("%d", &r);

    printf("Digite o número do termo que deseja encontrar (n): ");
    scanf("%d", &n);

    an = a1 + (n - 1) * r;

    printf("O %d-ésimo termo da PA é: %d\n", n, an);

    return 0;
}
i)#include <stdio.h>
#include <math.h>

int main() {
    float a, b, c, delta, x1, x2;

    printf("Digite o coeficiente a: ");
    scanf("%f", &a);

    printf("Digite o coeficiente b: ");
    scanf("%f", &b);

    printf("Digite o coeficiente c: ");
    scanf("%f", &c);

    delta = b * b - 4 * a * c;

    if (delta < 0) {
        printf("Não existem raízes reais.\n");
    } else if (delta == 0) {
        x1 = -b / (2 * a);
        printf("Raiz única: %.2f\n", x1);
    } else {
        x1 = (-b + sqrt(delta)) / (2 * a);
        x2 = (-b - sqrt(delta)) / (2 * a);
        printf("Raízes: x1 = %.2f, x2 = %.2f\n", x1, x2);
    }

    return 0;
}
j)#include <stdio.h>

int main() {
    int n1, n10, n50, n100, total;

    printf("Digite a quantidade de notas de 1 real: ");
    scanf("%d", &n1);

    printf("Digite a quantidade de notas de 10 reais: ");
    scanf("%d", &n10);

    printf("Digite a quantidade de notas de 50 reais: ");
    scanf("%d", &n50);

    printf("Digite a quantidade de notas de 100 reais: ");
    scanf("%d", &n100);

    total = n1 * 1 + n10 * 10 + n50 * 50 + n100 * 100;

    printf("O valor total em dinheiro é: %d reais\n", total);

    return 0;
}
k)#include <stdio.h>

int main() {
    int valor, n100, n50, n10, n5, n1;

    printf("Digite o valor em reais: ");
    scanf("%d", &valor);

    n100 = valor / 100;
    valor %= 100;

    n50 = valor / 50;
    valor %= 50;

    n10 = valor / 10;
    valor %= 10;

    n5 = valor / 5;
    valor %= 5;

    n1 = valor;

    printf("Notas necessárias:\n");
    printf("%d nota(s) de 100 reais\n", n100);
    printf("%d nota(s) de 50 reais\n", n50);
    printf("%d nota(s) de 10 reais\n", n10);
    printf("%d nota(s) de 5 reais\n", n5);
    printf("%d nota(s) de 1 real\n", n1);

    return 0;
}
