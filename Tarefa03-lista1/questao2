i)#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main() {
double a, b, c, delta, x1, x2;
printf("Digite o coeficiente a: ");
scanf("%lf", &a);
printf("Digite o coeficiente b: ");
scanf("%lf", &b);
printf("Digite o coeficiente c: ");
scanf("%lf", &c);
delta = ((b*b)-4*a*c);

if (delta < 0) {
printf("Não existem raízes reais.\n");} 

else if (delta == 0) {
x1 = ((-b )/ (2 * a));
printf("Raiz única: %.2f\n", x1);} 

else {
x1 = ((-b+(sqrt(delta)))/(a*2));
x2 = ((-b-(sqrt(delta)))/(a*2));
printf("Raízes: x1 = %.2lf, x2 = %.2lf\n", x1, x2);}
return 0;
}

j)#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main() {
int n1, n10, n50, n100, total;
printf("Digite a quantidade de notas de 1 real: ");
scanf("%d", &n1);
printf("Digite a quantidade de notas de 10 reais: ");
scanf("%d", &n10);
printf("Digite a quantidade de notas de 50 reais: ");
scanf("%d", &n50);
printf("Digite a quantidade de notas de 100 reais: ");
scanf("%d", &n100);
total = (n1+(n10*10)+(n50*50)+(n100*100));
printf("O valor total em dinheiro é: %d reais\n", total);
return 0;
}

l)#include <stdio.h>

int main() {
int valor, n100, n50, n10, n5, n1;
printf("Digite o valor em reais: ");
scanf("%d", &valor);
n100 = (valor / 100);
n50 = (valor / 50);
n10 =( valor / 10);
n5 = (valor / 5);
n1 = valor;
printf("Notas necessárias:\n");
printf("A quantidade de 1 real:%d\n De 10 reais:%d\n De 50 reais:%d\n De 100 reais:%d",n1,n10,n50,n100);
return 0;
}
