a)#include <studio.h>
#include <stdlib.h>

int main(){
int num1,num2,num3;
printf(digite o primeiro numero :");
scanf("%d",&num1);
printf("digite o segundo numereo:");
scanf("%d",num2);
printf("digite o segundo numereo:");
scanf("%d",num3);
printf("ordem inversa:%d,d%,%d\n",num3,num2,num1);
return 0;
}

